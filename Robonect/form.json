{
  "elements":
  [
    {
      "type": "Image",
      "image": "data:image/png;base64,"
    },
    { "type": "Label", "caption": "Disclaimer" },

    { "type": "Label", "caption": "   " },
    { "type": "Label", "label": "Login" },
    { "type": "ValidationTextBox", "name": "IPAddress", "caption": "IP-Adresse", "validate": "^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$" },
    { "type": "ValidationTextBox", "name": "Username", "caption": "Username" },
    { "type": "PasswordTextBox", "name":  "Password", "caption":  "Password" },

    { "type": "Label", "caption": "   " },
    { "type": "Label", "label": "AutomaticUpdate" },
    { "type": "CheckBox", "name": "HTTPUpdateTimer", "caption":  "AutomaticUpdate" },
    { "type": "NumberSpinner", "name": "UpdateTimer", "caption":  "UpdateTimer", "minimum":  10, "maximum":  300, "suffix":  "seconds" },

    { "type": "Label", "caption": "   " },
    { "type": "Label", "label": "MQTT Topic Label" },
    { "type": "ValidationTextBox", "name": "MQTTTopic", "caption":  "MQTTTopic" },

    { "type": "Label", "caption": "   " },
    { "type": "Label", "label": "DefaultValues" },
    { "type": "NumberSpinner", "name": "MowingTime", "caption":  "MowingTime", "minimum":  10, "maximum": 780, "suffix":  "minutes" }
  ],
  "actions":
  [
    { "type": "Button", "label": "Check Connection", "onClick": "Robonect_Update($id);" }
  ],
  "status":
  [
    { "code": 102, "icon": "active",      "caption": "Robonect identified" },
    { "code": 200, "icon": "inactive",    "caption": "Configuration missing" },
    { "code": 201, "icon": "error",       "caption": "IP invalid" },
    { "code": 202, "icon": "error",       "caption": "No device found on IP" },
    { "code": 203, "icon": "error",       "caption": "No JSON returned from IP" },
    { "code": 204, "icon": "error",       "caption": "No Robonect available at IP" }
  ]
}